.OrderRedPacket {
  min-height: 100vh;
  background-color: #101724;
  padding-bottom: 138px;
  box-sizing: border-box;
  padding-bottom: 200px;
  padding-top: 78px;
}
.OrderRedPacket .homeIcon {
  position: fixed;
  bottom: 120px;
  right: 40px;
  display: block;
  width: 100px;
  height: 100px;
  z-index: 3;
}
.OrderRedPacket .banner {
  display: block;
  width: 100%;
}
.OrderRedPacket .block-title {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 37px 0;
}
.OrderRedPacket .block-title:after {
  position: absolute;
  content: '';
  top: 50%;
  left: 0;
  width: 100%;
  background: #35445E;
  height: 2px;
  z-index: 1;
}
.OrderRedPacket .block-title .txt {
  color: #FFFFFF;
  font-size: 24px;
  background-color: #1E2A3F;
  line-height: 48px;
  height: 48px;
  text-align: center;
  padding: 0 20px;
  z-index: 2;
}
.OrderRedPacket .main {
  box-sizing: border-box;
  margin: 20px;
  background-color: #1E2A3F;
  border-radius: 20px;
  padding: 40px 30px;
}
.OrderRedPacket .main .title {
  text-align: center;
  color: #FFFFFF;
  font-size: 24px;
  margin-bottom: 40px;
}
.OrderRedPacket .main .coupon-empty {
  color: #1E3468;
  font-size: 30px;
  position: relative;
  z-index: 2;
}
.OrderRedPacket .main .packet-view {
  width: 647px;
  height: 260px;
  margin-bottom: 16px;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 60px 37px;
}
.OrderRedPacket .main .packet-view .redpacket {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 1;
}
.OrderRedPacket .main .packet-view .packet-left {
  width: 400px;
  position: relative;
  z-index: 2;
}
.OrderRedPacket .main .packet-view .packet-left .info {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #1E2A3F;
  font-size: 30px;
}
.OrderRedPacket .main .packet-view .packet-left .info .info-left .man {
  color: #564A3B;
  font-size: 24px;
  margin-top: 20px;
}
.OrderRedPacket .main .packet-view .packet-left .info .money {
  color: #D73B3B;
  font-size: 58px;
}
.OrderRedPacket .main .packet-view .packet-left .time {
  margin-top: 39px;
  height: 36px;
  line-height: 36px;
  background: #fdfdfd;
  border-radius: 18px;
  padding: 0 32px;
  color: #1E2A3F;
  font-size: 22px;
}
.OrderRedPacket .main .packet-view .packet-right {
  flex-shrink: 0;
  position: relative;
  z-index: 2;
}
.OrderRedPacket .main .packet-view .packet-right .btn {
  display: block;
  width: 96px;
  height: 99px;
}
.OrderRedPacket .main .coupon-view {
  position: relative;
  width: 642px;
  height: 191px;
  padding: 37px 37px 0;
  box-sizing: border-box;
  margin-bottom: 30px;
}
.OrderRedPacket .main .coupon-view .couponBg {
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.OrderRedPacket .main .coupon-view .coupon-info {
  position: relative;
  z-index: 2;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.OrderRedPacket .main .coupon-view .coupon-info .coupon-type {
  color: #1E2A3F;
  font-size: 30px;
  margin-bottom: 19px;
}
.OrderRedPacket .main .coupon-view .coupon-info .coupon-man {
  color: #564A3B;
  font-size: 24px;
}
.OrderRedPacket .main .coupon-view .coupon-info .coupon-money {
  color: #D73B3B;
  font-size: 58px;
  flex-shrink: 0;
}
.OrderRedPacket .main .coupon-view .coupon-time {
  color: #1E2A3F;
  font-size: 22px;
  position: absolute;
  bottom: 0;
  z-index: 2;
  line-height: 50px;
  height: 50px;
}
.OrderRedPacket .main .coupon-view:last-child {
  margin-bottom: 0;
}
.OrderRedPacket .receive-list {
  box-sizing: border-box;
  margin: 40px 20px 20px;
  background-color: #1E2A3F;
  border-radius: 20px;
  padding: 0 30px 40px 30px;
}
.OrderRedPacket .receive-list .list {
  display: flex;
  align-items: center;
  margin-bottom: 53px;
}
.OrderRedPacket .receive-list .list .thumb {
  display: block;
  flex-shrink: 0;
  width: 88px;
  height: 88px;
  margin-right: 42px;
  border-radius: 50%;
}
.OrderRedPacket .receive-list .list .receive-info {
  width: 100%;
}
.OrderRedPacket .receive-list .list .user {
  margin-bottom: 19px;
}
.OrderRedPacket .receive-list .list .user,
.OrderRedPacket .receive-list .list .time-view {
  overflow: hidden;
  font-size: 24px;
  color: #fff;
}
.OrderRedPacket .receive-list .list .user .item-left,
.OrderRedPacket .receive-list .list .time-view .item-left {
  float: left;
}
.OrderRedPacket .receive-list .list .user .item-right,
.OrderRedPacket .receive-list .list .time-view .item-right {
  float: right;
}
.OrderRedPacket .receive-list .list .user .time,
.OrderRedPacket .receive-list .list .time-view .time {
  color: #546179;
  float: left;
}
.OrderRedPacket .receive-list .list .user .best,
.OrderRedPacket .receive-list .list .time-view .best {
  float: right;
  width: 138px;
  height: 35px;
  text-align: center;
  line-height: 35px;
  background: #1e3468;
  border-radius: 18px;
  color: #fff;
}
.OrderRedPacket .receive-list .list:last-child {
  margin-bottom: 0;
}
.OrderRedPacket .rule-view {
  box-sizing: border-box;
  margin: 40px 20px 20px;
  background-color: #1E2A3F;
  border-radius: 20px;
  padding: 0 30px 40px 30px;
}
.OrderRedPacket .rule-view .txt {
  font-size: 22px;
  font-weight: 400;
  color: #ffffff;
  line-height: 48px;
}
.OrderRedPacket .share {
  padding: 0;
  margin: 0;
  width: 100%;
  height: 100px;
  line-height: 100px;
  background: #1e3468;
  color: #fff;
  font-size: 30px;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 3;
  text-align: center;
  border: none;
  outline: none;
}
.OrderRedPacket .share:after {
  border: none;
}
.PacketLoaging {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
