.newUserCouponWrap {
  position: fixed;
  top: 100%;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 883;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.4);
}
.newUserCouponWrap .newUserCoupon {
  position: fixed;
  z-index: 883;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  transform: translate3d(0, 100%, 0);
  transition: transform 0.25s cubic-bezier(0, 0, 0.25, 1);
}
.newUserCouponWrap .newUserCoupon .couponMain {
  background: #fff;
  width: 708px;
  border-radius: 5px;
  padding: 35px 26px;
  box-sizing: border-box;
  position: relative;
  z-index: 884;
  text-align: center;
}
.newUserCouponWrap .newUserCoupon .couponMain .closeIcon {
  display: block;
  width: 40px;
  height: 40px;
  margin-bottom: 2px;
}
.newUserCouponWrap .newUserCoupon .couponMain .title-view {
  color: #1E3468;
  font-size: 30px;
}
.newUserCouponWrap .newUserCoupon .couponMain .line {
  display: block;
  margin: 10px auto 0;
  width: 270px;
  height: 2px;
}
.newUserCouponWrap .newUserCoupon .couponMain .subhead {
  color: #313131;
  font-size: 22px;
  margin-top: 20px;
}
.newUserCouponWrap .newUserCoupon .couponMain .detail {
  width: 181px;
  height: 41px;
  border: 2px dashed #1e3468;
  line-height: 41px;
  text-align: center;
  color: #1E3468;
  font-size: 24px;
  margin: 22px auto 0;
}
.newUserCouponWrap .newUserCoupon .couponMain .coupon-list {
  display: flex;
  flex-wrap: wrap;
  margin-top: 29px;
}
.newUserCouponWrap .newUserCoupon .couponMain .coupon-list .list {
  width: 207px;
  height: 145px;
  position: relative;
  margin: 0 5px 18px;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.newUserCouponWrap .newUserCoupon .couponMain .coupon-list .list .couponBg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 9999;
}
.newUserCouponWrap .newUserCoupon .couponMain .coupon-list .list .num {
  color: #fff;
  font-size: 26px;
  z-index: 10000;
  position: relative;
}
.newUserCouponWrap .newUserCoupon .couponMain .coupon-list .list .enough,
.newUserCouponWrap .newUserCoupon .couponMain .coupon-list .list .type {
  color: #fff;
  font-size: 16px;
  margin-top: 9px;
  z-index: 10000;
  position: relative;
  width: 100%;
}
.newUserCouponWrap .newUserCoupon .couponMain .coupon-list .list .type {
  margin-top: 8px;
  padding-top: 10px;
}
.newUserCouponWrap .newUserCoupon .couponMain .coupon-list .list .type:before {
  content: '';
  position: absolute;
  width: 70%;
  background: #fff;
  height: 2px;
  left: 15%;
  top: 0;
}
.newUserCouponWrap .newUserCoupon .couponMain .receive {
  width: 276px;
  height: 66px;
  line-height: 66px;
  text-align: center;
  color: #1E3468;
  font-size: 30px;
  border: 1px solid #1e3468;
  margin: 34px auto 0;
}
.newUserCouponWrap .couponShow {
  transform: translate3d(0, 0, 0);
  transition: transform 0.25s cubic-bezier(0, 0, 0.25, 1);
}
.couponMaskShow {
  top: 0;
}
